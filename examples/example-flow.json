    {
        "id": "9e306515d48ac59b",
        "type": "function",
        "z": "b6c0ee55f6b2838a",
        "name": "convertOpenWeatherData",
        "func": "function iconToColor(icon) {\n    const condition = icon.substr(0, 2)\n    const dayNight = icon.substr(2, 1)\n    var result = '#abcabc'\n    /*\n    switch (condition) {\n        case '01': // clear sky\n            result = (dayNight === 'd')\n                ? 'rgb(255,255,0)'\n                : 'rgb(150,150,100)'\n            break\n        case '02': // few clouds\n            result = 'rgb(150,150,150)'\n            break\n        case '03': // scattered clouds\n            result = 'rgb(150,150,150)'\n            break\n        case '04': // broken clouds\n            result = 'rgb(150,150,150)'\n            break\n        case '09': // shower rain\n            result = 'rgb(50,50,150)'\n            break\n        case '10': // rain\n            result = 'rgb(100,100,150)'\n            break\n        case '11': // thunderstorm\n            result = 'rgb(200,200,0)'\n            break\n        case '13': // snow\n            result = 'rgb(150,150,150)'\n            break\n        case '50': // mist\n            result = 'rgb(150,150,150)'\n            break\n        default:\n            result = '#ffffff'\n    }*/\n\n    return result\n}\n\nfunction mapIcon(icon) {\n    const condition = icon.substr(0, 2)\n    const dayNight = icon.substr(2, 1)\n    var result\n    switch (condition) {\n        case '01': // clear sky\n            result = (dayNight === 'd') ?\n                'weather-sunny' : 'weather-night'\n            break\n        case '02': // few clouds\n            result = 'partly-cloudy'\n            break\n        case '03': // scattered clouds\n            result = 'weather-cloudy'\n            break\n        case '04': // broken clouds\n            result = 'partly-cloudy'\n            break\n        case '09': // shower rain\n            result = 'weather-pouring'\n            break\n        case '10': // rain\n            result = 'weather-rainy'\n            break\n        case '11': // thunderstorm\n            result = 'weather-lightning'\n            break\n        case '13': // snow\n            result = 'weather-snowy'\n            break\n        case '50': // mist\n            result = 'weather-fog'\n            break\n    }\n\n    if (!result.startsWith('weather')) {\n        result = 'weather-' +\n            ((dayNight == 'n') ? 'night-' : '') +\n            result\n    }\n\n    return result\n}\n\nfunction buildData(item, long=false) {\n    var tempC = Number(item.tempC)\n    \n    if (!isNaN(tempC)) {\n        tempC = (long) ? tempC.toFixed(1) : Math.round(tempC)\n        tempC +='Â°'\n    } else {\n        tempC = ''\n    }\n\n    const result = {\n        entityId: '',\n        value: tempC,\n        text: item.weekday || '',\n        icon: mapIcon(item.icon),\n        iconColor: iconToColor(item.icon)\n    }\n    return result\n}\n\nvar res = []\nconst data = msg.payload\nres.push(buildData(data.current, true))\n\nfor (var i=0; i<4 && i<data.forecast.length; i++)  {\n    const fcast = buildData(data.forecast[i])\n    if ( i==0 ) {\n        fcast.text='Heute'\n    }\n    res.push(fcast)\n}\n\nreturn {\n    topic: 'data',\n    payload: res\n}",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 200,
        "y": 340,
        "wires": [["a57e173da303f179"]]
    },
